        <arguments>
          <argument>
            <kind>1</kind>
            <string>if (!is_cinematic_displayed())
{
    if (just_killed_owl)
    {
        differ_drop=true;
        instance_destroy();
    }
    else
    {
        Target()
        alarm[1] = 10+random(10)
        if target &gt; 0
        {
            if collision_line(x,y,target.x,target.y,obj_Wall,0,0) &lt; 0
            {
                if point_distance(x,y,target.x,target.y) &lt; sight_range
                {          
                    if (random(1)&lt;0.5)
                    {       
                        if (!killed_dark_merlin)
                        {      
                            if (random(1)&lt;0.4)
                            {
                                display_ingame_message(x,y,"Show me your power, hero!",2,true,id);
                            }
                            else
                            {
                                display_ingame_message(x,y,"Allorah fiera palla!",2,true,id);
                            }
                        }
                        else
                        {
                            display_ingame_message(x,y,"...",2,true,id);
                            Play_Sound(snd_heal,0.3,0.8,0,3000,true,false);
                        }
                    }
                    wepangle = -wepangle
                    gunangle = point_direction(x,y,target.x,target.y)
                    if elite
                        ammo = 8;
                    else
                        ammo = 4;            
                    alarm[2] = 1;
                    alarm[1] = 60*5
                    motion_add(random(360),0.4)
                    walk = 40+random(15)
                    alarm[1] = walk+60+random(60)            
                }else 
                if random(3)&lt;1
                {
                    //TELEPORT
                    Play_Sound(snd_teleport,1,0.5,0,5000,true,false);
                    motion_add(gunangle,6)
                    wepangle = -wepangle
                    gunangle = point_direction(x,y,target.x,target.y)        
                    alarm[1] = 60                         
                    x = instance_furthest(x,y,obj_Clearer).x;
                    y = instance_furthest(x,y,obj_Clearer).y;             
                }else
                if random(5)&lt;1
                {
                    //SHOCKBALL
                    if (random(1)&lt;0.5)
                    {
                        if (!killed_dark_merlin)
                        {
                            display_ingame_message(x,y,"Dodge this!",2,true,id);
                        }
                        else
                        {
                            display_ingame_message(x,y,"...",2,true,id);
                            Play_Sound(snd_heal,0.3,0.8,0,3000,true,false);
                        }
                    }
                    motion_add(gunangle,6)
                    wepangle = -wepangle
                    gunangle = point_direction(x,y,target.x,target.y)        
                    alarm[1] = 30*3
                    var pos=0;
                    repeat(11)
                    {  
                        Create_Projectile(x,y,obj_Fireball,team,2,0,damage,dam_bon,mg_bon,mg_dam,8,gunangle+pos,99,0,false);
                        pos+=32            
                    }
                }else
                if random(3)&lt;1 and !instance_exists(obj_PWizard)
                {
                    spawn = 8;
                    alarm[3] = 2;
                    alarm[1] = 60;
                }
                if point_distance(x,y,target.x,target.y) &gt; sight_range+32
                {   
                    direction = point_direction(x,y,target.x,target.y)
                    //Smoke(4,1,360,depth,0.1,c_gray,3);
                    scr_part_smoke(4,360,random(4),.9,depth,3,c_gray,0,1);
                    dash = 10+random(5);
                    alarm[1] = 60+random(60)
                }
            }    
            else if random(5) &lt; 1
            {
                motion_add(random(360),0.4)
                walk = 40+random(15)
                alarm[1] = walk+60+random(60)
            }
        }
        else if random(10) &lt; 1
        {
            motion_add(random(360),0.4)
            walk = 40+random(15)
            alarm[1] = walk+20+random(60)
        }
    }
}
else
{
    alarm[1]=1;
}
