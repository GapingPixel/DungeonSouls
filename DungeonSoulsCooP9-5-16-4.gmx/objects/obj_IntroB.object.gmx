        instance_create(x,y,obj_FadeOut);
        room_goto_next();
        
   }   
}

if ( introNext = false ) then exit;
if ( fadeIn == true ) {
    image_alpha += 0.05;
    if ( image_alpha &gt;= 1.5 ) {
        fadeIn = false;
        fadeOut = true;
    }
} else if ( fadeOut == true ) {
    image_alpha -= 0.007;
    if ( (image_alpha &lt;= 0 )) {
        instance_create(x,y,obj_FadeOut);
        room_goto_next();
    }
}

if (stop_animation) {
    image_index = sprite_get_number(sprite_index)-1;
}




