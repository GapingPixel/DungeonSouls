        <arguments>
          <argument>
            <kind>1</kind>
            <string>if (!is_cinematic_displayed())
{
    Target()
    alarm[1] = 5+random(2)
    if target &gt; 0
    {
        if collision_line(x,y,target.x,target.y,obj_Wall,0,0) &lt; 0
        {
            if point_distance(target.x,target.y,x,y) &lt; 32 and random(2) &lt; 1
            { 
                //TELEPORT
                /*if (random(1)&lt;0.5)
                {
                    display_ingame_message(x,y,"Come here!",1,false,id);
                }*/
                Play_Sound(snd_teleport,1,0.5,0,5000,true,false);
                motion_add(gunangle,6)
                wepangle = -wepangle
                gunangle = point_direction(x,y,target.x,target.y)        
                if hp&lt;=maxhp/2 then alarm[1] = 30; else alarm[1] = 60
                x = instance_furthest(x,y,obj_Clearer).x;
                y = instance_furthest(x,y,obj_Clearer).y;                   
            }else
            if point_distance(target.x,target.y,x,y) &lt; 64 and point_distance(target.x,target.y,x,y) &gt; 16 and (random(8))&lt; 1
            {
                //ATTACK     
                gunangle = point_direction(x,y,target.x,target.y)   
                wepangle = -wepangle
                Create_Projectile(x+lengthdir_x(4,gunangle),y+lengthdir_y(4,gunangle),obj_Skeleton_King_Slash,team,.5,0,damage,dam_bon,mg_dam,mg_bon,8,gunangle,99,0,false);
                alarm[1] = 60+random(30)
            }else
            if point_distance(target.x,target.y,x,y) &gt; sight_range/2
            {    
                if random(5)&lt;1        
                {
                    if (random(1)&lt;0.5)
                    {
                        stop_last_voice();
                        Play_Sound(snd_skeleton_king_shock_gun,1,1.5,1,5000,true,false);
                        display_ingame_message(x,y,choose("Let's see you dance!"),2,true,id);
                        last_voice=snd_skeleton_king_shock_gun;
                    }
                    mode = choose(0,1);
                    ammo = 8;
                    sprite_index=spr_fire;
                    image_index=0;
                    override_animation=true;
                    alarm[2] = 5;
                    alarm[1] = 60;
                }else if random(5)&lt;1
                {
                    //SHOCKBALL
                    motion_add(gunangle,6)
                    wepangle = -wepangle
                    sprite_index=spr_fire;
                    image_index=0;
                    override_animation=true;
                    gunangle = point_direction(x,y,target.x,target.y)        
                    alarm[1] = 30*3
                    var pos=0;
                    repeat(11)
                    {  
                        Create_Projectile(x,y,obj_Shock_Ball,team,2,0,damage,dam_bon,mg_dam,mg_bon,8,gunangle+pos,99,0,false);
                        pos+=32            
                    }
                }else if random(3)&lt;1 and !instance_exists(obj_Skeleton_Warrior)
                {
                    spawn = 8;
                    var _m=irandom_range(1,2);
                    var _text,_dur;
                    switch (_m)
                    {
                        case 1:
                            _text="Come forth, minions!";
                            _dur=2;
                            stop_last_voice();
                            Play_Sound(snd_skeleton_king_summon_b,1,1.5,1,5000,true,false);
                            last_voice=snd_skeleton_king_summon_b;
                        break;
                        case 2:
                            if (obj_Hero.attack_type[global.choice]== "NIGHT" || obj_Hero.attack_type[global.choice]== "ARROW" || obj_Hero.attack_type[global.choice]== "KNIFE")
                            {
                                _text="Kill her!";
                                stop_last_voice();
                                Play_Sound(snd_skeleton_king_summon_a_female,1,1.5,1,5000,true,false);
                                last_voice=snd_skeleton_king_summon_a_female;
                            }
                            else
                            {
                                _text="Kill him!";
                                stop_last_voice();
                                Play_Sound(snd_skeleton_king_summon_a,1,1.5,1,5000,true,false);
                                last_voice=snd_skeleton_king_summon_a;
                            }
                            _dur=1;
                        break;
                        default:
                            _text="Come forth, minions!";
                            _dur=2;
                            stop_last_voice();
                            Play_Sound(snd_skeleton_king_summon_b,1,1.5,1,5000,true,false);
                            last_voice=snd_skeleton_king_summon_b;
                        break;
                    }
                    display_ingame_message(x,y,_text,_dur,true,id);
                    alarm[3] = 2;
                    alarm[1] = 60;
                }else if random(2)&lt;1
                {
                    direction = point_direction(x,y,target.x,target.y)
                    dash = 50+random(10);
                    alarm[1] = 60*3;
                }else if random(3)&lt;1
                {
                    //TELEPORT
                    Play_Sound(snd_teleport,1,0.5,0,5000,true,false);
                    motion_add(gunangle,6)
                    wepangle = -wepangle
                    gunangle = point_direction(x,y,target.x,target.y)        
                    alarm[1] = 60                         
                    x = instance_furthest(x,y,obj_Clearer).x;
                    y = instance_furthest(x,y,obj_Clearer).y;             
                }
                else
                {
                    Play_Sound(snd_cast,.9,0.3,0,3000,true,false);
                    var pat = choose(1,2,3);
                    if pat == 1
                    {
                        obj = instance_create(target.x+lengthdir_x(16,0),target.y+lengthdir_y(16,0),obj_Fire_Burst_Spot_Enemy);obj.creator = id;
                        obj = instance_create(target.x+lengthdir_x(16,90),target.y+lengthdir_y(16,90),obj_Fire_Burst_Spot_Enemy);obj.creator = id;
                        obj = instance_create(target.x+lengthdir_x(16,180),target.y+lengthdir_y(16,180),obj_Fire_Burst_Spot_Enemy);obj.creator = id;
                        obj = instance_create(target.x+lengthdir_x(16,270),target.y+lengthdir_y(16,270),obj_Fire_Burst_Spot_Enemy);obj.creator = id;
                    }else
                    if pat == 2
                    {
                        obj = instance_create(target.x-16,target.y,obj_Fire_Burst_Spot_Enemy);obj.creator = id;                
                        obj = instance_create(target.x,target.y,obj_Fire_Burst_Spot_Enemy);obj.creator = id;
                        obj = instance_create(target.x+16,target.y,obj_Fire_Burst_Spot_Enemy);obj.creator = id;
                        obj = instance_create(target.x,target.y-16,obj_Fire_Burst_Spot_Enemy);obj.creator = id;                
                        obj = instance_create(target.x,target.y+16,obj_Fire_Burst_Spot_Enemy);obj.creator = id;                    
                    }else
                    if pat == 3
                    {
                        obj = instance_create(target.x-32,target.y-32,obj_Fire_Burst_Spot_Enemy);obj.creator = id;                
                        obj = instance_create(target.x-16,target.y-16,obj_Fire_Burst_Spot_Enemy);obj.creator = id;                
                        obj = instance_create(target.x,target.y,obj_Fire_Burst_Spot_Enemy);obj.creator = id;
                        obj = instance_create(target.x+16,target.y+16,obj_Fire_Burst_Spot_Enemy);obj.creator = id;
                        obj = instance_create(target.x+32,target.y+32,obj_Fire_Burst_Spot_Enemy);obj.creator = id;                    
    
                        obj = instance_create(target.x-32,target.y+32,obj_Fire_Burst_Spot_Enemy);obj.creator = id;                
                        obj = instance_create(target.x-16,target.y+16,obj_Fire_Burst_Spot_Enemy);obj.creator = id;                
                        obj = instance_create(target.x+16,target.y-16,obj_Fire_Burst_Spot_Enemy);obj.creator = id;
                        obj = instance_create(target.x+32,target.y-32,obj_Fire_Burst_Spot_Enemy);obj.creator = id;                                        
                    }
                    alarm[1] = 60*3;
                }         
            }else        
            {
            direction = point_direction(x,y,target.x,target.y)
            speed = 0.4
            walk = 40+random(10)
            alarm[1] = 10+random(4)
            }
        }    
        else if random(5) &lt; 1
        {
            motion_add(random(360),0.4)
            walk = 10+random(15)
            alarm[1] = walk+20+random(60)
        }else
        {
            //Smoke(4,1,360,depth,0.1,merge_color(c_black,c_dkgray,0.5),4); 
            scr_part_smoke(4,360,random(4),.9,depth,4,merge_color(c_black,c_dkgray,0.5),0,1);
            with instance_create(x,y,obj_Spawn) object = -1   
            with obj_Hero
            {
                var direc = Random(0,360);
                while(!tile_layer_find(100,x+lengthdir_x(32,direc),y+lengthdir_y(32,direc)))
                {
                    var direc = Random(0,360);
                }
                other.x = x+lengthdir_x(32,direc);
                other.y = y+lengthdir_y(32,direc);
            }
            Play_Sound(snd_teleport,0.9,.5,0,5000,true,false);  
            with instance_create(x,y,obj_Spawn) object = -1 
            alarm[0] = 60;
        }    
    }
    else if random(10) &lt; 1
    {
        motion_add(random(360),0.4)
        walk = 10+random(15)
        alarm[1] = walk+20+random(60)
    }
}
else
{
    alarm[1]=1;
}
