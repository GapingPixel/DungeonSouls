    {
        speed = 2+random(2)/2;
        direction = random(360);
        friction = 0.2;
        value = global.dungeon+1;
    }
}
repeat (3)
{
    var ing = choose(obj_Metal_Scrap,obj_Fire_Shard,obj_Magic_Dust,obj_Magic_Essence,obj_Ice_Shard);
    ing_val=1;
    switch (ing)
    {
        case obj_Metal_Scrap:
            ing_val=irandom_range(10,20);
        break;
        case obj_Fire_Shard:
            ing_val=irandom_range(2,3);
        break;
        case obj_Magic_Dust:
            ing_val=irandom_range(10,20);
        break;
        case obj_Magic_Essence:
            ing_val=irandom_range(5,10);
        break;
        case obj_Ice_Shard:
            ing_val=irandom_range(2,3);
        break;
    }
    
    ing_val*=global.loop;
    
    with (instance_create(x,y,ing))
    {
        speed = 2+random(2)/2;
        direction = random(360);
        friction = 0.2;
        value = other.ing_val
    }
}

