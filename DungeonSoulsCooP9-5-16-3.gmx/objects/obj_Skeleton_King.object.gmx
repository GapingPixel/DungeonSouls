    global.exp_col += other.expdrop;
    global.exp_ctr = 120
    //Popup("+"+string(floor(other.expdrop))+" exp",merge_colour(c_purple,c_white,0.4))    
}
if instance_number(obj_Skeleton_King) == 1 and !instance_exists(obj_Judgement2)
{
    instance_create(x,y,obj_Boss_Chest);
    if audio_is_playing(snd_skeleton_king)
        audio_sound_gain(snd_skeleton_king,0,5000);
}else
{
    with obj_Judgement
    {
        x = xorig;
        y = yorig;
    }
}

