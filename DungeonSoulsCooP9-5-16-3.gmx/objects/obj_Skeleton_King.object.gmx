    {
        ing = obj_Bone;
    
        ing_val=irandom_range(2,4)*global.loop;
        
        with (instance_create(x,y,ing))
        {
            speed = 2+random(2)/2;
            direction = random(360);
            friction = 0.2;
            value = other.ing_val
        }
    }
}

